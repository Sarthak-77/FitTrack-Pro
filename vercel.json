{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/css/(.*)",
            "dest": "/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/(.*\\.(html|png|jpg|jpeg|gif|svg|ico|webp))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}